{
  "name": "impostor_js",
  "version": "2.1.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "index.d.ts",
  "typesVersions": {
    ">=4.0": {
      "*": [
        "dist/types/*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": "Carl Beattie <12beattiecastp@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/carlbeattie2000/impostor.js"
  },
  "scripts": {
    "clean": "rimraf dist node_modules",
    "build:clean": "rimraf dist",
    "build:types": "tsc --project tsconfig.build.json",
    "build:code": "tsx ./scripts/bundle.ts",
    "build": "run-s build:clean build:types build:code",
    "test": "vitest",
    "format": "prettier --cache --write .",
    "release": "standard-version"
  },
  "keywords": [
    "nodejs",
    "fake_user_information",
    "fake_user_account_information",
    "json"
  ],
  "devDependencies": {
    "@types/node": "^20.10.6",
    "csv-parse": "^5.0.4",
    "esbuild": "^0.19.11",
    "eslint": "^8.6.0",
    "glob": "^10.3.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "standard-version": "^9.5.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.1"
  },
  "packageManager": "pnpm@8.12.1"
}
